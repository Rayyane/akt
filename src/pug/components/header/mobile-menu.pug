#slide-menu.sidenav.mobile-nav
  ul.collection
    each item, index in menu.fashion
      li.collection-item(style="animation-duration: "+(Number(index + 1)) * 0.25 + 's')
        a.sidenav-close.waves-effect.menu-list(href='#' + item.name) #{$t.fashion_landing['header_'+item.name]}
    li.collection-item(style="animation-duration: "+menu.fashion.length * 0.25 + 's')
        a.waves-effect.menu-list(href=link.fashion.contact) #{$t.fashion_landing.header_contact}